<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<!-- jdbc-type is name for java.sql.Types -->
		<!-- length, scale and precision can be used to specify the mapping precisely -->
		<!-- the type-mappings are ordered. This mapping will be consulted last, 
			thus overridden by the previous one if precision=1 for the column -->

		<sql-type jdbc-type="DATE" hibernate-type="date" />
		<sql-type jdbc-type="TIME" hibernate-type="time" />
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp" />
		<sql-type jdbc-type="BIT" hibernate-type="boolean" />
		<sql-type jdbc-type="TINYINT" hibernate-type="byte" />
		<sql-type jdbc-type="SMALLINT" hibernate-type="short" />
		<sql-type jdbc-type="INTEGER" hibernate-type="integer" />
		<sql-type jdbc-type="BIGINT" hibernate-type="long" />
		<sql-type jdbc-type="FLOAT" hibernate-type="float" />
		<sql-type jdbc-type="DOUBLE" hibernate-type="double" />
		<sql-type jdbc-type="NUMERIC" hibernate-type="big_decimal" />
		<sql-type jdbc-type="CHAR" hibernate-type="string" />
		<sql-type jdbc-type="CHAR" hibernate-type="character"
			length="1" />
		<sql-type jdbc-type="VARCHAR" hibernate-type="string" />
	</type-mapping>
	<table-filter match-name="tb_ElectricFee" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_ElectricFeeCharge"
		match-catalog="db_pmp2" match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_ElectricFeeItem"
		match-catalog="db_pmp2" match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_LiftMeterItem" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_OperateDetail" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_OwnerRepair" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_ProMeterItem" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_RepairAttach" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_RepairFee" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table-filter match-name="tb_BuilFeeRate" match-catalog="db_pmp2"
		match-schema="dbo" package="org.pmp.vo">
	</table-filter>
	<table catalog="db_pmp2" schema="dbo" name="tb_BuilFeeRate" class="BuilFeeRate"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_RepairAttach" class="RepairAttach"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_LiftMeterItem" class="LiftMeterItem"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_ElectricFee" class="ElectricFee"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_OwnerRepair" class="OwnerRepair"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_ElectricFeeItem" class="ElectricFeeItem"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_ProMeterItem" class="ProMeterItem"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_OperateDetail" class="OperateDetail"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_RepairFee" class="RepairFee"></table>
	<table catalog="db_pmp2" schema="dbo" name="tb_ElectricFeeCharge" class="ElectricFeeCharge"></table>
</hibernate-reverse-engineering>