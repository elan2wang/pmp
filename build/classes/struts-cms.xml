<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
  <constant name="struts.i18n.encoding" value="UTF-8" />
  
  <package name="cms" extends="struts-default" namespace="/cms">
	<!-- SMS_Company related Actions -->
	<action name="addSMSCompany" class="org.pmp.action.business.SMSCompanyAction" method="addSMSCompany">
      <result name="success" type="chain">loadSMSCompanyList</result>
    </action>
    
    <action name="getSMSCompany" class="org.pmp.action.business.SMSCompanyAction" method="getSMSCompany">
      <result name="success">/cms/smsc_edit.jsp</result>
    </action>
    
    <action name="editSMSCompany" class="org.pmp.action.business.SMSCompanyAction" method="editSMSCompany">
      <result name="success" type="chain">loadSMSCompanyList</result>
    </action>
    
    <action name="deleteSMSCompany" class="org.pmp.action.business.SMSCompanyAction" method="deleteSMSCompany">
      <result name="success" type="chain">loadSMSCompanyList</result>
    </action>
    
    <action name="loadSMSCompanyList" class="org.pmp.action.business.SMSCompanyAction" method="loadSMSCompanyList">
      <result name="success">/cms/smsc_list.jsp</result>
    </action>
    
    
	<!-- company related actions -->
	<action name="select_company_list" class="org.pmp.action.business.CompanyAction" method="select_company_list">
	</action>
	<action name="getCompanyByID" class="org.pmp.action.business.CompanyAction" method="getCompanyByID">
	  <result name="success">/cms/cmpy_edit.jsp</result>
	</action>
	<action name="addCompany" class="org.pmp.action.business.CompanyAction" method="addCompany">
	  <result name="success">/cms/cmpy_list.jsp</result>
	</action>
	<action name="updateCompany" class="org.pmp.action.business.CompanyAction" method="updateCompany">
	  <result name="success">/cms/cmpy_list.jsp</result>
	</action>
	<action name="deleteCompany" class="org.pmp.action.business.CompanyAction" method="deleteCompany">
	  <result name="success">/cms/cmpy_list.jsp</result>
	</action>
	<action name="cmpy_list" class="org.pmp.action.business.CompanyAction" method="loadCompanyList">
	</action>
	
	<action name="companyAction" class="org.pmp.action.business.CompanyAction">
		<result name="success">/cms/cmpySelect.jsp </result>
	</action>
	<action name="building_list" class="org.pmp.action.business.BuildingAction" method="loadBuildingList">
	</action>
	<action name="project_list" class="org.pmp.action.business.ProjectAction" method="loadProjectList">
	</action>
	<action name="deleteProject" class="org.pmp.action.business.ProjectAction" method="deleteProject">
	</action>
	<action name="get_building" class="org.pmp.action.business.BuildingAction" method="getBuildingById">
		<result name="success">/cms/building_edit.jsp</result>
	</action>
	<action name="delete_building" class="org.pmp.action.business.BuildingAction" method="deleteBuilding">
	</action>
	<action name="buildingAction" class="org.pmp.action.business.BuildingAction" method="loadBuildingList">
		<result name="success">/cms/building_list.jsp</result>
	</action>
	<action name="getBuildingByProject" class="org.pmp.action.business.BuildingAction" method="getBuildingByProject">
	</action>
	<action name="get_project" class="org.pmp.action.business.ProjectAction" method="getProjectById">
		<result name="success">/cms/project_edit.jsp</result>
	</action>
	<action name="getAllProject" class="org.pmp.action.business.ProjectAction" method="getAllProject">
		<result name="success">/cms/projectSelect.jsp</result>
	</action>
	<action name="getProjectBySessionHander" class="org.pmp.action.business.ProjectAction" method="getProjectBySessionHander">
		<result name="success">/cms/projectSelect.jsp</result>
	</action>
	
	<action name="getProjectByDistrict" class="org.pmp.action.business.ProjectAction" method="getProjectByDistrict">
	</action>
	<action name="update_Project" class="org.pmp.action.business.ProjectAction" method="updateProject">
		<result name="success">/cms/project_list.jsp</result>
	</action>
	<action name="projectAction" class="org.pmp.action.business.ProjectAction" method="">
		<result name="success">/cms/project_list.jsp</result>
	</action>
	<action name="house_list" class="org.pmp.action.business.HouseAction" method="loadHouseList">
	</action>
	<action name="getHouseByConditions" class="org.pmp.action.business.HouseAction" method="getHouseByProjectOrBuilding">
	</action>
	<action name="getHouseById" class="org.pmp.action.business.HouseAction" method="getHouseById">
		<result name="success">/cms/house_edit.jsp</result>
	</action>
	<action name="getAllHouseNum" class="org.pmp.action.business.HouseAction" method="getAllHouseNum"></action>
	<action name="deleteHouse" class="org.pmp.action.business.HouseAction" method="deleteHouse"></action>
	
	
	<action name="fileUpload" class="org.pmp.action.business.ProjectAction" method="uploadFile">
		<result name="success">/cms/project_list.jsp</result>
	</action>
	<action name="buildingImport" class="org.pmp.action.business.BuildingAction" method="uploadFile">
		<result name="success">/cms/project_list.jsp</result>
	</action>
	
	<!-- owner related actions -->
	<action name="loadOwnerList_ByPro" class="org.pmp.action.business.OwnerAction" method="loadOwnerList_ByPro" />
    
    <action name="addOwner" class="org.pmp.action.business.OwnerAction" method="addOwner">
      <result name="success" type="redirect">/cms/owner_list.jsp</result>
    </action>
    
    <action name="editOwner" class="org.pmp.action.business.OwnerAction" method="editOwner">
      <result name="success" type="redirect">/cms/owner_list.jsp</result>
    </action>
    
    <action name="getOwner" class="org.pmp.action.business.OwnerAction" method="getOwnerInfo">
      <result name="success">/cms/owner_edit.jsp</result>
    </action>
    
    <!-- s:action tag related actions  -->
    <action name="load_owner_list_banner" class="org.pmp.action.business.PageTriggerAction" method="select_project">
      <result name="success">/fee/pro_options.jsp</result>
    </action>
  </package>

</struts>
  